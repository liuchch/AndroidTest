<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="打开miui省电管理"
        android:onClick="on1Clicked"
        />
    <com.example.liu.myapplication.view.NewTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NewTextView"
        />

    <com.example.liu.myapplication.view.TopBar
        android:layout_width="match_parent"
        android:layout_height="80dp"
        custom:leftBackground="@color/colorPrimaryDark"
        custom:leftText="Back"
        custom:leftTextColor="@color/colorAccent"
        custom:rightText="go"
        custom:rightTextColor="@color/colorPrimaryDark"
        custom:rightBackground="@color/colorAccent"
        custom:title="title"
        custom:titleTextColor="@color/colorPrimary"
        custom:titleTextSize="20dp"
        />




</LinearLayout>